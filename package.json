    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "esm-env": "^1.0.0",
    "flowbite": "^1.8.1",
    "flowbite-svelte": "^0.44.2",
    "postcss": "^8.4.28",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "vanilla-lazyload": "^17.8.4",
    "vite": "^4.4.9",

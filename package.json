{
  "name": "daks-nic",
  "version": "1.4.0",
  "description": "NIC",
  "repository": {
    "type": "git",
    "url": "https://github.com/daks-dev/daks-nic"
  },
  "author": {
    "name": "DVA",
    "email": "mail@daks.dev",
    "url": "https://daks.dev"
  },
  "homepage": "https://dev.stroyexp.info/",
  "private": false,
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "start": "node build/index.js",
    "cluster": "node run/cluster.js",
    "dev": "vite dev --port=3000",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "create:sitemap": "node run/create.sitemap.js",
    "create:iconset": "node run/create.iconset.js",
    "create:images": "node run/create.images.js",
    "build": "vite build",
    "preview": "vite preview --port=3000",
    "test": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.22.6",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "autoprefixer": "^10.4.15",
    "classnames": "^2.3.2",
    "cssnano": "^6.0.1",
    "cssnano-preset-advanced": "^6.0.1",
    "daks-svelte": "^0.4.5",
    "dotenv": "^16.3.1",
